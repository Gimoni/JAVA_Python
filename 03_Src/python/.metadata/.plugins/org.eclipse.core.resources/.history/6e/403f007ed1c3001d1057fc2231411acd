
#
# 1.             2.            3.           4.
# *                *           *****           *
# **              **           ****           **
# ***            ***           ***           ***
# ****          ****           **           ****
# *****        *****           *           *****

print("1.")

for y in range(5):
    print("*")
        

print("2.")

for y in range(5):
    for x in range(5):
        print("*")

print ("Program End...")
