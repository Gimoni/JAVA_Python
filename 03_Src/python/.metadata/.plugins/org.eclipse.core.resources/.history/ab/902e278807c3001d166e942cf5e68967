

print([False])
print(type[False])

print("="*50)
print([False]*40)
print([[False]*40]*20)      # 20*40 20행 40열

rect = [[False]*40]*20;     # 20*40

print("rect len= ", len(rect))          #20
print("rect[0] len= ", len(rect[0]))    #40

for i in range(len(rect)):
    for j in range(len(rect[i])):
        print("%d," % rect[i][j])       # True :1 , False:0
    print()






    
print("Program End...")