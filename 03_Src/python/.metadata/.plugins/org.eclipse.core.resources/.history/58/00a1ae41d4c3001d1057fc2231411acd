
#
# 1.             2.            3.           4.
# *                *           *****           *
# **              **           ****           **
# ***            ***           ***           ***
# ****          ****           **           ****
# *****        *****           *           *****
stars = ['*****','****','***','**','*']
stars2 = ['*','**','***','****','*****']



print(".-1")

while stars:
    print (stars.pop()) 
    
print(".-2")

while stars2:
    print (stars2.pop()) 
  

print("1.")

for y in range(5):
    for x in range(5):
        
        print("*", end="")
    print()
        

print("2.")
for y in range(5):
    for x in range(5):
        print("*", end="")
    print()

print("3.")
for y in range(5):
    for x in range(5):
        print("*", end="")
    print()
    

    
print("4.")
for y in range(5):
    for x in range(5):
        print("*", end="")
    print()
print ("Program End...")
