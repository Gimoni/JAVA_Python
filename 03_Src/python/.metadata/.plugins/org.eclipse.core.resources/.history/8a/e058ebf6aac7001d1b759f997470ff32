
def modify1(v):
    print("v : ", v, id(v))
    v += "###"
    print("v : ", v, id(v))
    
value = "Hello"

print("전 :", value, id(value))
modify1(v=value) 
print("후 :", value, id(value))

print("#"*20)

def modify2(v):
    v += [10]*3
    
value = [1,2,3,4]

print("전 :", value, id(value))
modify2(v=value)
print("후 :", value, id(value))

def modify3(v):
    v += 20
    
value = 10

print("전 :", value, id(value))
modify3(v=value)
print("후 :", value, id(value))


    







    


print("Program End...")