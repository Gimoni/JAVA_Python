# test example
from util.VT100 import clearScreen, reset

clearScreen()

year = 2023
month = 3
day = 20

# date = '2023-03-20'
date = f'{year:04d}-{month:02d}-{day:02d}'

for i in range(len(date)):
    ch = date[i]        # 저장 
    # print(date[i])
    print(ch)

print()
print(date)




reset()

print("Program End")