import time
from datetime import datetime
# from pandas._libs.tslibs.offsets import Week

def printDate(year, month, day):
    current = time.time();
    
    while(True):
        year = 1970
        week = 4
        dayOfYear = 365
        if(year%4==0 and year%100!=0 or year%400==0):
            dayOfYear+=1
            
        secondOfYear = dayOfYear*24*60*60
        
        if (int(current)-secondOfYear >=0):
            current -= secondOfYear
            week += dayOfYear
        else: 
            dayOfYear
        
        year +=1
    return year 

    month = 0
    for i in range(1,13):
        dayOfMonth =[31,28,31,30,31,30,31,31,30,31,30,31]
        if(year%4==0 and year%100!=0 or year%400==0):
            dayOfYear[1] = 29
    
    secondOfMonth = dayOfMonth*24*60*60
    if (int(current)-secondOfMonth >= 0):
        int(current) -= secondOfMonth
        week += dayOfMonth
    else : 
        month = i
        break
    return month
    
    day = int(current)/60/60/24
    week += day
    return day
    return week

year, month, day = printDate(year, month, day)


def getWeekDay():
    current = time.time();
    week=printDate.week
    if (int(current) - printDate.day*24*60*60 >0 ):
        printDate.day+=1
    # week = week%7 
    weekName = ['MON','TUE','WED','THU','FRY','SAT','SUN']
    return week
