from time import time
from datetime import datetime
from pandas._libs.tslibs.offsets import Week

def printDate(year, month, day):
    current = time();
    year = 1970
    week = 4
    
    while(True):
        dayOfYear = 365
        if(year%4==0 and year%100!=0 or year%400==0):
            dayOfYear+=1
            
        secondOfYear = dayOfYear*24*60*60
        
        if (current-secondOfYear >=0):
            current -= secondOfYear
            week += dayOfYear
        else: 
            dayOfYear
        
        year +=1
    return year 

    month = 0
    for i in range(1,13):
        dayOfMonth =[31,28,31,30,31,30,31,31,30,31,30,31]
        if(year%4==0 and year%100!=0 or year%400==0):
            dayOfYear[1] = 29
    
    secondOfMonth = dayOfMonth*24*60*60
    if (current-secondOfMonth >= 0):
        current -= secondOfMonth
        week += dayOfMonth
    else : 
        month = i
        # break
    
    return month
    
    day = current/60/60/24
    week += day
    
    if (current - day*24*60*60 >0 ):
        day+=1
    return day

# return "%04d-%02d-%02d" % (year, month, day)


