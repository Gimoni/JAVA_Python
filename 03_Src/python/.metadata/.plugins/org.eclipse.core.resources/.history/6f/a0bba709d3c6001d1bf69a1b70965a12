from random import randrange

result = ''


# 8 자리 비밀번호 .. 
for i in range(8):
    group = randrange(0, 3)
    ch = ''
    if group==0:        # upper case   
        ch = randrange(ord('A'), ord('Z')+1) 
        pass
    elif group==1:      # lower case
        ch = randrange(ord('a'), ord('z')+1) 
        pass
    elif group==2:      # numder
        ch = randrange(ord('0'), ord('9')+1) 
        pass
    
    result += chr(ch)
    
print(result)

print("Program End... ")