
b1 = 100
s1 = 200
c1 = 'A'    # 문자열
i1 = 300
l1 = 400 

f1 = 10.5
d1 = 10.5

tf = True

print("b1 = ", type(b1))    # int
print("s1 = ", type(s1))    # int
print("c1 = ", type(c1))    # str
print("i1 = ", type(i1))    # int
print("l1 = ", type(l1))    # int

print("f1 = ", type(f1))    # int
print("d1 = ", type(d1))    # int

print("tf = ", type(tf))    # int

# python 에서는 정수타입 int 와 실수타입 
