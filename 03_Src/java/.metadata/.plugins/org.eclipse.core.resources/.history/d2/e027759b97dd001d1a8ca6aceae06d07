package com.example;

public class ContraintTypeParameterExample {

	static class A {
		
	}
	
	static class B extends A {
		
	}
	
	static class C {
		
	}
	
	static class Box<T> extends A { // 제한된 타입 파라미터 /// A와 A를 상속받은 B만 올수 있음 .
		T contenet;
	}
	
	public static void main(String[] args) {
		
		
	}

}
