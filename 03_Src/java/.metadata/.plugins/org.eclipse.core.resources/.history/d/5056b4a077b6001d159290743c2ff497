
public class NumberStringExample {

	public static void main(String[] args) {
		int n = 1; 		// 0000 0001 
		
		char ch = 1;
		
		System.out.println(ch);
		
	}
	
	public static void main2(String[] args) {

		int num = 100;				// (4byte) 0110 0100      숫자 이진수 100이 num에 저장됨. 
		
		
		System.out.println(num);	
		
	}

}
