/*window.onload = function(e) {
	console.log("onload..." + e.type);
}*/

// class 

class Alpha {
	constructor() {
		window.addEventListener("load", this);
		
	}
	
	handleEvent(e) {
		console.log("handleEvent..." + e.type);
		this[e.type](e);
	}	
	
	load() {	// 나 자신을 불러옴. 
		document.forms.alpha.addEventListener("submit", this);	
	}
}

new Alpha();
