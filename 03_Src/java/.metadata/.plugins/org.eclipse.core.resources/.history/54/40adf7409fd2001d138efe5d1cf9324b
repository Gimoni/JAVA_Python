select * from emp;

select ename from emp;

select ename, job from emp;

--select from where 절  
select ename, job, gender 
	from emp
 where gender ='F';


select ename, job, gender, deptno
	from emp
 where deptno = 10;
 
 
select ename 이름, job 직급, gender 성별, deptno 부서번호
	from emp
 where deptno = 10;
 
 select ename "이 름", job 직급, gender 성별, deptno 부서번호
	from emp
 where deptno = 10;