package com.example.ex;

public class EX14_3 {
	interface Max {
		int max(int a, int b);
	}
	interface S {
		String s (String Hello);
	}
	
	
	public static void main(String[] args) {
		// 익명 구현 객체
		S s1 = new S() {
			
			@Override
			public String s(String Hello) {
				return s(Hello).toS;
			}
		};
		// 람다식
		Max m2 = (a, b) -> a>=b ? a:b;
		
		System.out.println(m2.max(10, 20));
	}

}
