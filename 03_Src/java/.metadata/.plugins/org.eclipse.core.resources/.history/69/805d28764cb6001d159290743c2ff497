
public class VariableExample {
	/*
	 * 식별자 (Identifier)
	 * 		- 글자 갯수 제한 없음
	 * 		- 문자(영어+한글)
	 * 		- 숫자 (0~9)
	 * 		- 특수문자 _,$ 가능
	 * 
	 * 		- 첫문자는 반드시 ( 문자, _, $ ) 만 가능 / 숫자가 오면 안됨
	 * 		-
	 */
	
	public static void main(String[] args) {
		
		int 대한민국 = 10;
		int num1 = 100;
		int num2 = 200;
		int num___ = 300;
		int num$$$ = 400;
//		int num% = 500; / error
		
		
//		int 9num = 100; // error
		int _num = 100; 
		int $num = 200;
		
		
		
		
	}

}
