package com.example;
// 인터페이스 상속. 
public class InheritanceExample {

	interface Moveable {
		void move();
	}
	
	interface Drawable extends Runnable, Moveable{
		void draw();
	}
	
	class A implements Drawable {

		@Override
		public void run() {
			// TODO Auto-generated method stub
			
		}

		@Override
		public void draw() {
			// TODO Auto-generated method stub
			
		}

		@Override
		public void move() {
			// TODO Auto-generated method stub
			
		}
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
