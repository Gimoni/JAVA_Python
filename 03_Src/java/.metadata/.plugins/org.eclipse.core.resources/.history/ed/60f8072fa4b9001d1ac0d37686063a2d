
public class QuizControlStatementsExample {

	public static void main(String[] args) {
		int num = 1;
		for (num=1; num<11; num++) {
			System.out.println(num + (num++));
		}
	}
	public static void main2(String[] args) {
		// [연습문제 4-1]
		int i ;
		for (i=1; i<=20; i++) {
			if (!(i%2 ==0 && i%3==0)) {
				i++;
			}
		}
		System.out.println(i);
	}

}
