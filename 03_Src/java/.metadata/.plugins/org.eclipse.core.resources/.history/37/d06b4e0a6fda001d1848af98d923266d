package com.example;

public class AnonymousExample2 {

	interface Drawable {
		void draw();
	}
	
	static class A implements Drawable {

		@Override
		public void draw() {
			System.out.println("A..");
			
		}
		
	}
	
	public static void main(String[] args) {
		Drawable d1 = new A();
		
	}

}
