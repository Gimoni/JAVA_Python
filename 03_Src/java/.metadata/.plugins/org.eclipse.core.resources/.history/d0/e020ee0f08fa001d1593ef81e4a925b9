
package com.example.model;

import com.example.util.Gender;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class Emp {
	int empno;
	String ename;
	Gender gender;
	String job;
	Integer mgr;
	String hiredate;
	int sal;
	int comm;
	int deptno;
}


// Null 도 저장할 수 있는 Integer Mapper type 사용 ---> mgr 