/*
callback function



*/

function triangle(predict) {
	for(let i=0; i<5; i++) {
		for (let j=0; j<5; j++) {
			if (predict(i, j))
				process.stdout.write("*");
			else
				process.stdout.write(" ");
		}
		console.log("");
	}	
}

function number1(i, j) {
	return i>=j;
}
triangle(number1);