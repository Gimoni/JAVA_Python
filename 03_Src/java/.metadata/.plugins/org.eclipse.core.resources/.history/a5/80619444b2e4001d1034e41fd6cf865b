/*
callback function



*/

// callback 함수 선언 
function triangle(predict) {
	
	// 반복문 
	for(let i=0; i<5; i++) {
		for (let j=0; j<5; j++) {
			// 매개변수로 전달된 함수 호출
			if (predict(i, j))
				process.stdout.write("*");
			else
				process.stdout.write(" ");
		}
		console.log("");
	}	
}

function number1(i, j) {
	return i>=j;
}
triangle(number1);	// callback 함수 호출 . 