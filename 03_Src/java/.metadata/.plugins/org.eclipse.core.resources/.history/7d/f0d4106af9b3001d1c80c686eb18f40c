
public class IntegerExample {
	/*
	 * 정수 타입
	 *  1. byte		1byte
	 *  2. short	2byte
	 *  3. char		2byte
	 *  4. int 		4byte
	 *  5. long		8byte
	 *  
	 *  
	 *  Q.
	 *  1. 38(10) => 2진수로 변환
	 *  
	 *  0010 0110
	 *  
	 *  
	 *  1. -38(10) => 2진수로 변환
	 *  
	 *  0010 0110
	 *  1101 1001
	 *  
	 */
	public static void main(String[] args) {
		// 정수 타입
		byte b;
		short s;
		char c;
		int i;
		long l;
		
		b=0b0010_0110; // 38(10)
		System.out.println(b);
	}

}
