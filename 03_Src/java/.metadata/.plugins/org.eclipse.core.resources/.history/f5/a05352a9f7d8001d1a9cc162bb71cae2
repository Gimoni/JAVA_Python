package com.example.mapper;

public class AlphaMapper {
	
	// 
	public int selectCountByLineColumn(int line, int column) {
		
		String sql = """
				select count(*)
				  from alpha
				 where line = ? and col = ? 
				""";
		
		int rtn = 0;
		
		return rtn;
	}
	
	public int selectAll() {
		
		String sql = """
				select count(*)
				  from alpha
				""";
		
		int rtn = 0;
		
		return rtn;
	}
}
