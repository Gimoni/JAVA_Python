package com.example;

import java.util.HashSet;
import java.util.TreeSet;

public class TreeSetExample {

	public static void main(String[] args) {
		String str1 = "java";
		String str2 = "python";
		System.out.println(str1.equals(str2)); 	// 동등비교 ==
		System.out.println(str1.compareTo(str2));	// 대소비교 str1 - str2
		
	}
	
	
	
	public static void main3(String[] args) {
		var set1 = new HashSet<String>();
		var set2 = new TreeSet<String>();
		//String의 compareto를 사용해서 비교함
		// 문자열의 비교 기준에 따라서 값을 리턴 함 작은것부터~
		
		set1.add("java");
		set1.add("python");
		set1.add("html");
		set1.add("css");
		set1.add("javascript");
		
		set2.add("java");
		set2.add("python");
		set2.add("html");
		set2.add("css");
		set2.add("javascript");
		
		System.out.println(set1);
		System.out.println(set2);
	}
	
	
	public static void main2(String[] args) {
		var set1 = new HashSet<Integer>();
		var set2 = new TreeSet<Integer>();
		
		for (int i=0; i<10; i++) {
			set1.add(i);
			set2.add(i);
		}
		
		System.out.println(set1);
		System.out.println(set2);
		
		
		
		
		
	}

}
