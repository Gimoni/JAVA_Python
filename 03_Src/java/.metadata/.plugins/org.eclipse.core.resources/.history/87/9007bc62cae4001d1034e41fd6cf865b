/**
 *  함수별 호출 차이 확인 
 *  function의 this. 객체는 자기자신. 
 */

class Alpha {
	line = 10;
	column = 10;
	constructor() {
		let x = function() {
			console.log("x() = " + this.line)
		}
		let y = () => {
			console.log("y() = " + this.line)
		}
		try {
			x();
		} catch (e) {
			
		}		
		y();
	}
}

new Alpha();