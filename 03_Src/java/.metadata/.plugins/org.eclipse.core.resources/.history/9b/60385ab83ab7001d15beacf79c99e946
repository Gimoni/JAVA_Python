import java.awt.Color;

public class RectangleExample {

	public static void main(String[] args) {
		Turtle t = new Turtle();
		Turtle.setCanvasSize(600, 600);		// 
		
		t.speed(1000);
		t.penColor(Color.yellow);
		
		t.up();
		t.setPosition(-200, 0); 			// 절대좌표위치로
		
		
		t.down();
		t.forward(400);
		
		
	}

}
