package com.example;

import java.util.ArrayList;
import java.util.LinkedList;
import java.util.List;

public class PerformanceExample {

	static class ListPerformance {
		int size;
		List<Integer> list;
		
		ListPerformance(int size, List<Integer> list) {
			this.size = size;
		}
		
		void create() {
			
			for(int i=0; i<size; i++) {
				list.
			}
		}
	}
	
	public static void main(String[] args) {
//		ListPerformance<Integer> a = 
//				new ListPerformance<>(500000, new ArrayList<Integer>());
		var a =  new ListPerformance<Integer>(500000, new ArrayList<Integer>());
		var b =  new ListPerformance<Integer>(500000, new LinkedList<Integer>());

	}

}
