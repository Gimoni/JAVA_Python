
public class GuGuDanExample {

	public static void main(String[] args) {
		for(int i=1; i<10; i++) {
			System.out.printf("%d X %d = %d\n", 2, i, 2*i );
			System.out.printf("%d X %d = %d\n", 3, i, 3*i );
			System.out.printf("%d X %d = %d\n", 4, i, 4*i );
			System.out.printf("%d X %d = %d\n", 5, i, 5*i );
			System.out.printf("%d X %d = %d\n", 6, i, 6*i );
			System.out.printf("%d X %d = %d\n", 7, i, 7*i );
			System.out.printf("%d X %d = %d\n", 8, i, 8*i );
			System.out.printf("%d X %d = %d\n", 9, i, 9*i );
		}
	}
	public static void main2(String[] args) {
		
		for(int i=1; i<10; i++) {
			System.out.println(2*i);
			System.out.println(3*i);
			System.out.println(4*i);
			System.out.println(5*i);
			System.out.println(6*i);
			System.out.println(7*i);
			System.out.println(8*i);
			System.out.println(9*i);
		}
		
	}

}
