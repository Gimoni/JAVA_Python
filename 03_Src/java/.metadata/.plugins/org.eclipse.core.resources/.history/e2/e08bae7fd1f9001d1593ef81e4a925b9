package com.example;

import static org.junit.Assert.assertTrue;

import java.util.Random;

import org.junit.jupiter.api.Test;

public class Random3Test {

	@Test
	void random() {
		for (int i=0; i<100; i++) {
			double value = Math.random();
		
			// 단정문 assert
			assertTrue(value>=0);
			assertTrue(value< 0.8);
		}
		
		
		
	}
}
