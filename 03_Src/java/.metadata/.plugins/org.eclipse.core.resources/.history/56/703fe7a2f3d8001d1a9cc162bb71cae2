package com.example;

public class ThrowExample {

	public static void main(String[] args) {
		
		try {
			xxx();
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		try {
			yyy();
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
	static void xxx() throws Exception {
		throw new Exception();
	}
	
	static void yyy() throws Exception {
		throw new Exception();
	}
	
}
