
public class ContinueBreakExample {

	
	public static void main(String[] args) {
		
		int sum = 0; // 3의 배수의 합을 구하기
		
		for (int i=1; i<=100; i++) {
			
			if (i%3!=0)
				continue;
			
			sum += i;
			
		}
		
		System.out.println(sum);
	}
	public static void main2(String[] args) {

		int cnt =0;
		for(int i=0; i<100; i++) {
			cnt++;
			if (i == 9)
				break;
		}
		
		System.out.println(cnt);
	}

}
