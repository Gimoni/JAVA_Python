	static void printMonth(int year, int month) {
		long epoch = System.currentTimeMillis()/1000;
		
		int dayOfYear = (year%4==0 && year%100!=0 || year%400==0) ? 366 : 365;
		int dayOfMonth = year - (dayOfYear*24*60);
		
