package com.example.ex;

public class EX14_3 {
	interface Max {
		int max(int a, int b);
	}
	interface F {
		int f(int a, int b);
	}
	
	
	public static void main(String[] args) {
		// 익명 구현 객체
		Max total6 = new Max() {
			
			@Override
			public int max(int a, int b) {
				return a+b<=6 ? a:b;
			}
		};
		// 람다식
		Max m2 = (a, b) -> a>=b ? a:b;
		
		System.out.println(total6);
	}

}
